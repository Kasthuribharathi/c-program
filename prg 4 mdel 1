#include<stdio.h>
int main(void)
{
	int rem,sum=0,i,num;
	printf("Enter the number");
	scanf("%d",&num);
	for(i=1;i<num;i++)
	{
		rem=num%i;
		if(rem==0)
		{
			sum=sum+i;
			
		}
	
	}
		if (sum==num)
			printf("The number %d is a perfect number",num);
		
		else
		
			printf("The number %d is not a pefect number",num);
		
}
